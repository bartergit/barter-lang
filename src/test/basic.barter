func inc(int x) int {
    int res = x + 1
    return res
}
func sum(int x, int y) int {
    x = x + y
    return x
}
func is_positive(int x) int {
    if x < 0 {
        return 0
    }
    return 1
}
func main() void {
    int res = inc(5)
    int c = sum(res,res)
    log(res)
    log(c)
}
