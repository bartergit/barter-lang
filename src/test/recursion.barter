func recursive(bool flag_value) int {
    if flag_value {
        return 1
    } else {
        return recursive(true) + recursive(true)
    }
}
func fact(int number) int {
    if number <= 1 {
        return 1
    }
    return number * fact(number-1)
}
func main() void {
    print(recursive(false) + recursive(true))
    print(fact(5))
}