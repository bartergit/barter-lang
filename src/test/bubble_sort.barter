func swap (int first int second) void {
    int temp = deref(first)
    set_ref(first,deref(second))
    set_ref(second,temp)
    return
}
func inc (int x) int {
    return sum(x,1)
}
func print_array (int arr int size) void {
    int i = 0
    while lt(i,size) {
        cout(deref(sum(arr,i)))
        i = inc(i)
    }
    line()
    return
}
