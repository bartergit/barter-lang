dec_func('swap', 'void',
    block(
        set_arg('arr', 'ref')
    ),
    block(
        dec_var('tmp', 'int', index(deref('arr'),0)),
        set_arr(deref('arr'),0, index(deref('arr'),1)),
        set_arr(deref('arr'),1, tmp),
        ret_void()
    )
);
