func do_nothing(int a) void {
}

func do_another_nothing() void {
    return
}

func do_sec_nothing(int b, int c) void {
    log(b)
    log(c)
    return
}

func do_something() void {
    log(1)
}

func inc(int arg) int {
    arg = arg + 1
    return arg
}

func main() void {
    int one = 1
    int two = 2
    do_nothing(one)
    do_another_nothing()
